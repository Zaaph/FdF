#include <mlx.h>

int	main()
{
	void	*mlx;
	void	*win;
	char	*title;

	mlx = mlx_init();
	title = "fenetre test";
	mlx_new_window(mlx, 200, 200, title);
	mlx_loop(mlx);
	return (0);
}
